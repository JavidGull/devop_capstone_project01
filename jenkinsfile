pipeline {
    agent any
    stages {
       stage('Branch Develop Git Checkout Code') {
          when { branch 'develop'
          beforeAgent true}
          agent {label 'dev'}
          steps {
               git branch: 'develop', url: 'https://github.com/JavidGull/devop_capstone_project01.git'  
             }
         }
    }
}
